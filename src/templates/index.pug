---
title: Agrinorm
---
extends layouts/_layout
block content
	.main
		+header('active')
		.main__center.center
			.main__wrap
				h1.main__title.title(data-aos)
					p <span>Smart</span> <span>Quality</span>
					p <span>Management</span>
					p <span>Platform.</span>
				.main__info(data-aos="animation-translate-up" data-aos-delay="200") We help fresh produce suppliers exceed customer expectations while reducing claims and waste.
				.main__btn(data-aos="animation-translate-up" data-aos-delay="400")
					button.btn.btn_primary Contact Us
				a.main__scroll.js-scroll(href="#details" data-aos="animation-translate-up" data-aos-delay="1000")
					.main__icon
						.main__arrows
							img.main__pic(src="img/arrow-down.svg" alt="")
							img.main__pic(src="img/arrow-down.svg" alt="")
					.main__text Scroll down
			.main__bg(data-aos)
				// phone
				.main__preview 
					img.main__pic.js-parallax(data-scale="1.4" data-orientation="up" src="img/main-pic-1.png" alt="")
				// pie chart
				.main__preview
					img.main__pic.js-parallax(data-scale="1.2" data-orientation="down" src="img/main-pic-2.png" alt="")
				// bar chart
				.main__preview
					img.main__pic.js-parallax(data-scale="1.2" data-orientation="up" src="img/main-pic-3.png" alt="")
				// hand
				.main__preview
					//- img.main__pic.js-parallax(data-scale="0.8" data-orientation="down" src="img/main-pic-4.png" alt="")
				// blue triangle
				.main__preview
					img.main__pic.js-parallax(data-scale="1.2" data-orientation="right" src="img/case-figure.svg" alt="")
					//- img.main__pic.js-parallax(data-scale="1.2" data-orientation="right" src="img/main-pic-5.svg" alt="")
				// grid
				.main__preview
					//- img.main__pic.js-parallax(data-scale="1.4" data-orientation="down" src="img/main-pic-6.png" alt="")
				// apple
