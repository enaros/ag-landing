	.details.js-details#details
		.bg
			.bg__preview
				img.bg__pic.js-parallax(data-scale="1.3" data-orientation="left" src="img/bg.svg" alt="")
		.details__center.center
			.details__head.head
				.head__box
					.head__stage.stage(data-aos="animation-translate-right") 
